@using CW_W12_114.Utiliti;
@{
    var data = ViewBag.data as Student;
}
<div class="card" style="width: 18rem;">
    <div class="card-body">
        <h5 class="card-title">@data.FirstName</h5>
        <h6 class="card-subtitle mb-2 text-body-secondary">@data.LastName</h6>
        <p class="card-text">@data.GetFullName()</p>
        <a href="#" class="card-link">Card link</a>
        <a href="#" class="card-link">Another link</a>
    </div>
</div>
